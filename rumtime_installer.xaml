<fw:AcrylicWindow x:Class="System_Init_Toolbox.net_framework"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:System_Init_Toolbox"
        mc:Ignorable="d"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        Title="运行库安装器" Height="300" Width="350" Icon="/appicon.png">
    <Grid>
        <ui:NavigationView>
            <ui:NavigationView.MenuItems>
                <ui:NavigationViewItem Content="一键安装" x:Name="OneClickInstall" />
                <ui:NavigationViewItem Content="自定义安装" x:Name="Install" />
            </ui:NavigationView.MenuItems>
        </ui:NavigationView>
    </Grid>
</fw:AcrylicWindow>
